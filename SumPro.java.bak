import java.util.*;
class  SumPro
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		
		while(t-->0)
		{
			int n = sc.nextInt();
			int arr[]=new arr[n];
			for(int i =0;i<n;i++)
				arr[i]=sc.nextInt();
	int ans = 1;
     
       for (int i = 0; i < n; i++)
		   {
           if (arr[i] < 0) {
               System.out.println("Invalid Input");
               return;
           }
           ans *= (arr[i] + 1);
			 }
      
       System.out.println(ans - 1);
	}
	}
}
